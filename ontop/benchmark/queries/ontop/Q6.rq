PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX geof: <http://www.opengis.net/def/function/geosparql/>
PREFIX uom: <http://www.opengis.net/def/uom/OGC/1.0/>
PREFIX : <http://www.semanticweb.org/alpano/ontologies/2020/3/
gdm_vkg#>

SELECT * WHERE {
# Find class
?a a :Bank .
# Retrieve geometry
?a :asWKT ?ag .
# Retrieve label
?a :label ?name .
# Filter non-language tagged labels and Italian labels
FILTER(lang(?aname) = "" || lang(?aname) = "de")
# Filter objects of class located inside in polygon
FILTER(geof:sfContains(’POLYGON((7.3 44.5, 8.5 44.5, 8.5 45.5,
7.3 45.5, 7.3 44.5))’^^geo:wktLiteral, ?ag))
}